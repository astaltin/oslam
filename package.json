{
  "name": "oslam-billing",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && pnpm \"db:generate prod\"",
    "start": "next start",
    "lint": "next lint",
    "shadcn-ui": "pnpx shadcn@2.*",
    "db:generate prod": "dotenv -e .env.production prisma generate",
    "db:migrate prod": "dotenv -e .env.production prisma db push",
    "db:migrate-fresh prod": "pnpm \"db:migrate-reset prod\" && pnpm \"db:migrate prod\" && pnpm \"db:seed prod\"",
    "db:migrate-reset prod": "dotenv -e .env.production tsx prisma/migrate-reset.mts",
    "db:seed prod": "dotenv -e .env.production tsx prisma/seeder.mts",
    "db:generate dev": "dotenv -e .env.development prisma generate --allow-no-models",
    "db:migrate dev": "dotenv -e .env.development prisma db push",
    "db:migrate-fresh dev": "pnpm \"db:migrate-reset dev\" && pnpm \"db:migrate dev\" && pnpm \"db:seed dev\"",
    "db:migrate-reset dev": "dotenv -e .env.development tsx prisma/migrate-reset.mts",
    "db:seed dev": "dotenv -e .env.development tsx prisma/seeder.mts"
  },
  "dependencies": {
    "@fontsource/geist-sans": "^5.1.0",
    "@prisma/client": "^5.21.1",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@types/pluralize": "^0.0.33",
    "axios": "^1.7.7",
    "change-case": "^5.4.4",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.453.0",
    "mongodb": "^6.10.0",
    "nanoid": "^5.0.7",
    "next": "15.0.1",
    "pluralize": "^8.0.0",
    "react": "19.0.0-rc-69d4b800-20241021",
    "react-dom": "19.0.0-rc-69d4b800-20241021",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.2.0",
    "@types/node": "^20",
    "@types/pluralize": "^0.0.33",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8",
    "eslint-config-next": "15.0.1",
    "postcss": "^8",
    "prisma": "^5.21.1",
    "prisma-json-types-generator": "^3.1.1",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.19.2",
    "typescript": "^5"
  }
}
